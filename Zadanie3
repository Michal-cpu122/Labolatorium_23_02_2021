package com.company;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Podaj Stopnie Celcjusza");
        int d = scanner.nextInt();
        System.out.println( 0.9 * (2 * d + 32));
        System.out.println();
    }}
